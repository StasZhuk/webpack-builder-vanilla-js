{
  "name": "twigBuilder",
  "version": "0.0.1",
  "description": "markup",
  "main": "webpack.config.js",
  "scripts": {
    "build:prod": "concurrently --raw --success last \"gulp\" \"cross-env NODE_ENV=production webpack  --progress --hide-modules --mode=production\"",
    "build:dev": "concurrently --raw --success last \"gulp\" \"webpack --progress --hide-modules --mode=development\"",
    "watch": "concurrently --raw --success last \"gulp watch\" \"webpack-dev-server --progress --hide-modules --mode=development\"",
    "hot": "concurrently --raw --success last \"gulp watch\" \"webpack-dev-server --hot --inline --mode=development\"",
    "hot:host": "concurrently --raw --success last \"gulp watch\" \" webpack-dev-server --hot --inline --host 0.0.0.0 --mode=development\""
  },
  "author": "Mihail Vedernikov, ZhukZhuk",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^8.6.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "bowser": "^1.9.3",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "gulp": "^4.0.2",
    "gulp-cheerio": "^0.6.3",
    "gulp-clean": "^0.4.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "gulp-svg-sprite": "^1.4.0",
    "gulp-watch": "^5.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "twig": "1.10.5",
    "twig-loader": "^0.4.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-spritesmith": "^0.4.1"
  },
  "dependencies": {
    "natives": "^1.1.6"
  }
}
